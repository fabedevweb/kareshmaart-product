efault {\n  name: \"Login\",\n  data: function () {\n    return {\n      mode: \"login\",\n      email: null,\n      password: null,\n      pseudoForm: \"\",\n      emailForm: \"\",\n      passwordForm: \"\",\n      emailRegex: \"\",\n    };\n  },\n  mounted: {\n    hideMenu() {\n      this.$emit();\n    },\n  },\n  computed: {\n    validatedFields: function () {\n      if (this.email != \"\" && this.password != \"\") {\n        return true;\n      } else {\n        return false;\n      }\n    },\n  },\n  methods: {\n    showLogin: function () {\n      this.mode = \"login\";\n    },\n    login: function () {\n      axios\n        .post(`http://localhost:3000/api/auth/login`, {\n          email: this.email,\n          password: this.password,\n        })\n        .then((res) => {\n          console.log(res.data, \"Connect√©\");\n          localStorage.setItem(\"admin\", JSON.stringify(res.data));\n          this.$router.push(\"/Create\");\n        });\n    },\n    validEmail: function (email) {\n      var re = /^[a-zA-Z0-9.!#$%&'*+/=?^_]+@[a-zA-Z0-9-]+(?:.[a-zA-Z0-9-]+)$/;\n      return re.test(email);\n    },\n  },\n};\n</script>\n\n<style scoped>\n.background {\n  font-family: new-order, sans-serif;\n  font-weight: 900;\n  font-style: normal;\n  font-size: 2rem;\n}\n.background-card {\n  padding-top: 200px;\n}\n.hello--img__icon {\n  position: relative;\n  width: 50%;\n  margin-left: 25%;\n  margin-top: 100px;\n}\n\n.card__action {\n  text-decoration: none !important;\n}\n.card {\n  max-width: 25%;\n  min-width: 350px;\n  margin-bottom: 100%;\n  background-color: transparent;\n}\n.form-row {\n  display: flex;\n  margin: 16px 0px;\n  gap: 16px;\n  flex-wrap: wrap;\n}\n.form-row__input {\n  padding: 8px;\n  border-radius: 8px;\n  font-weight: 500;\n  font-size: 16px;\n  flex: 1;\n  min-width: 100px;\n  color: white;\n  background-color: #ff8a00;\n}\n</style>\n"],"sourceRoot":"src/views"}]}