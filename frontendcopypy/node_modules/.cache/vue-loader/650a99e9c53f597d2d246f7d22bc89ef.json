ected: function () {\n      if (localStorage.getItem(\"admin\")) {\n        return true;\n      }\n    },\n    adminDisconnected: function () {\n      localStorage.removeItem(\"admin\");\n      this.$router.push(\"/admin\");\n    },\n  },\n};\n</script>\n\n<style>\n.realisations {\n  font-family: new-order, sans-serif;\n  font-weight: 200;\n  font-style: normal;\n  background-color: #ff8a00;\n}\nbody {\n  background-color: #e3f2fd;\n}\n\n.heading {\n  font-size: 20px;\n  font-weight: 600;\n  color: #3d5afe;\n}\n\n.line1 {\n  color: #000;\n  font-size: 12px;\n}\n\n.line2 {\n  color: #000;\n  font-size: 12px;\n}\n\n.line3 {\n  color: #000;\n  font-size: 12px;\n}\n\n.cards {\n  transition: all 0.2s ease;\n  cursor: pointer;\n}\n\n.cards:hover {\n  box-shadow: 5px 6px 6px 2px #e9ecef;\n  transform: scale(1.1);\n}\n</style>\n"]}]}